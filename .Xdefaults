! Normal
! color0    BLACK
! color1    red
! color2    green
! color3    yellow
! color4    blue 
! color5    purple 
! color6    cyan 
! color7    white

urxvt*color0:    #000000
urxvt*color1:    #C00000
urxvt*color2:    #00C000
urxvt*color3:    #C0C000
urxvt*color4:    #0C00C0
urxvt*color5:    #AA00AA
urxvt*color6:    #00AAAA
urxvt*color7:    #FFFFFF

! Light
! color8    darkgray
! color9    red
! color10    green
! color11    yellow
! color12    blue 
! color13    purple 
! color14    cyan 
! color15    lightgray

urxvt*color8:    #212121
urxvt*color9:    #FF8080
urxvt*color10:    #80FF80
urxvt*color11:    #FFFF88
urxvt*color12:    #99CCFF
urxvt*color13:    #B14CC2
urxvt*color14:    #9BFFFF
urxvt*color15:    #D0D0D0





