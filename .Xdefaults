# 
URxvt*scrollBar: false


URxvt*background: #0f1f1f
URxvt*foreground: #dbfef8


URxvt*perl-lib: /home/coon/.config/urxvt/perl/


#clickable urls
URxvt*perl-ext-common:  default,tabbedex,fullscreen,clipboard,url-select,
URxvt.matcher.button:	1
URxvt.url-launcher: /usr/bin/chromium

#fullscreen
*keysym.F11: perl:fullscreen:switch

#url-select without mouse
URxvt.urlLauncher:     firefox
URxvt.underlineURLs:   true
URxvt.keysym.M-u:      perl:url-select:select_next

